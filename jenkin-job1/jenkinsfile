pipeline{
    agent any
    parameters {
        
        
        string description: 'hello enter your name', name: 'firstname'
        string description: 'enter your last name', name: 'lastname'
    }
    stages{
        
        stage("welcome"){
            
          
            steps{
                
                echo "Hi ${params.firstname}, and your lastname ${params.lastname}"
        }
    }
    }
}
    



    
    


     
